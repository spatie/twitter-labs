{
    "name": "spatie/twitter-labs",
    "description": "PHP client for Twitter Labs endpoints",
    "keywords": [
        "spatie",
        "twitter-labs"
    ],
    "homepage": "https://github.com/spatie/twitter-labs",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Vanderbist",
            "email": "alex@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "clue/buzz-react": "^2.6.1",
        "react/promise": "^2.2",
        "league/oauth2-client": "^2.4",
        "spatie/data-transfer-object": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.2 | ^9.3.0",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\TwitterLabs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\TwitterLabs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    }
}